<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>transactions</servlet-name>
    <servlet-class>com.zohoinc.Transaction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>transactions</servlet-name>
    <url-pattern>/transactions</url-pattern>
  </servlet-mapping>
     <servlet>
  <servlet-name>New Account</servlet-name>
  <servlet-class>com.zohoinc.NewAccount</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>New Account</servlet-name>
    <url-pattern>/newAccount</url-pattern>
    </servlet-mapping>
     <servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>com.zohoinc.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>check for available customers</servlet-name>
    <servlet-class>com.zohoinc.AvailableCustomers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>check for available customers</servlet-name>
    <url-pattern>/availableCustomers</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Welcome</servlet-name>
    <servlet-class>com.zohoinc.Example</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
</web-app>
